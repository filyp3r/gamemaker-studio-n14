            <kind>1</kind>
            <string>Scp_Stuck();
if(global.pause){exit;}
if(!attack){
    sprite_index = Spr_ENSpeederIdle;
    image_speed = 0.1;
    if(collision_rectangle(x-rad/2,y-sprite_height/1.5,x+rad/2,y-3,Obj_Player,true,false)){
        if(Obj_Player.x &gt; x){move = 1;}
        if(Obj_Player.x &lt; x){move = -1;}
        if(place_meeting(x,y+1,Obj_Floor)){
            attack = true;
        }
    }
    hsp = 0;
    image_xscale = 1;
}else{
    vsp = 0;
    image_xscale = move;
    sprite_index = Spr_ENSpeederAttack;
    image_speed = 0.2;
    hsp += move*sp;
    if(hsp &gt;= maxsp){hsp = maxsp;}
    if(hsp &lt;= -maxsp){hsp = -maxsp;}
    if(place_meeting(x+move,y,Obj_Floor)){
        attack = false;
        vsp = 0;
        vsp -= 5;
        hsp -= image_xscale*10;
        Obj_Camera.hit = 10;
    }
}
//HColl
if(place_meeting(x+hsp,y,Obj_Floor)){
    while(!place_meeting(x+sign(hsp),y,Obj_Floor)){
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;
//VColl
if(place_meeting(x,y+vsp,Obj_Floor)){
    while(!place_meeting(x,y+sign(vsp),Obj_Floor)){
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;
vsp += grv;
if(vsp &gt;= 5){vsp = 5};

//LIFE
if(life &lt;= 0){
    instance_destroy();
}
</string>
