        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>image_index = irandom_range(0,7);
hsp = 0;
vsp = 0;
grv = 0.4;
image_speed = 0.1;
walksp = image_speed*4;
move = 1;
life = 10;
flashAlpha = 1;
maxTimeToAttack = random_range(100,170);
timeToAttack = 0;
attack = false;
numInst = 0;

RadSound = view_wview/2;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>Scp_Light(128,c_white);
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>Scp_Stuck();
if(global.pause){image_speed = 0; exit;}
if(x&lt;view_xview-50)or(x &gt; view_xview+view_wview+50){exit;}
if(y&lt;view_yview-50)or(y &gt; view_yview+view_hview+100){exit;}
//Sound3D
    var Emitter = audio_emitter_create();
    audio_emitter_position(Emitter,x,y,noone);
    if(collision_circle(x,y,RadSound,Obj_Player,true,false)){
        audio_emitter_gain(Emitter,1);
    }else{
        audio_emitter_gain(Emitter,0);
    }
//Attack
if(!attack){
    sprite_index = Spr_ENSworderWalk;
    image_speed = 0.1;
    walksp = image_speed*4;
    if(place_meeting(x,y,Obj_PHitBox)){
        if(Obj_Player.x &gt; x){move = 1;}
        if(Obj_Player.x &lt; x){move = -1;}
    }
    if(image_index &gt;= 3){
        if(move = 1){
            if(place_meeting(x+1,y,Obj_Floor)){
                move = -1;
            }
        }
        if(move = -1){
            if(place_meeting(x-1,y,Obj_Floor)){
                move = 1;
            }
        }
        hsp = move * walksp;
    }else{
        hsp = 0;
    }
    if(timeToAttack &lt;= 0){
        if(collision_rectangle(x+image_xscale*8,y-sprite_height/1.5,x+image_xscale*35,y-3,Obj_Player,true,false)){
            attack = true;
            image_index = 0;
        }
    }else{
        timeToAttack -= 1;
    }
}else{
    hsp = 0;
    sprite_index = Spr_ENSworderAttack;
    if(image_index &lt;= 3){
        image_speed = 0.1;
    }else if(image_index &lt;= 5){
        image_speed = 0.15;
    }else{
        image_speed = 0.05;
    }
    if(image_index &gt;= 8){
        attack = false;
        timeToAttack = maxTimeToAttack;
    }
    if(image_index &gt;= 4) and (image_index &lt;= 4.2){
        Obj_Camera.x += random_range(-5,5);
        Obj_Camera.y += random_range(-5,5);
        Scp_SworderAttack(Spr_ENSworderHitBox);
        audio_play_sound_on(Emitter,Snd_SFXEnemySwordFloor,0,0);
    }else{
        with(Obj_EnemyHitBox){
            if(sprite_index = Spr_ENSworderHitBox){
                instance_destroy();
            }
        }
    }
}
image_xscale = move;
//HColl
if(place_meeting(x+hsp,y,Obj_Floor)){
    while(!place_meeting(x+sign(hsp),y,Obj_Floor)){
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;
//VColl
if(place_meeting(x,y+vsp,Obj_Floor)){
    while(!place_meeting(x,y+sign(vsp),Obj_Floor)){
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;
vsp += grv;
if(vsp &gt;= 5){vsp = 5;}

//Life
if(life &lt;= 0){
    instance_destroy();
    Obj_Player.PUMana += 1;
    audio_play_sound(Snd_SFXEnemyDie,0,0);
    Obj_Camera.HIT = 10;
    with(instance_create(x,y,Obj_Death)){
        sprite_index = Spr_ENSworderDeath;
        image_speed = 0.2;
        image_xscale = other.image_xscale;
        maxFrame = 7;
    }
    if(instance_exists(Obj_EnemyHitBox)){
        with(Obj_EnemyHitBox){
            if(sprite_index = Spr_ENSworderHitBox){
                instance_destroy();
            }
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
